                    config="content=zero;containers=u(6,6);objects=u(300001,700000);sizes=c(512)KB" id="op3"/>
                <operation type="delete" ratio="30" division="none" afr="2000"
                    config="containers=u(6,6);objects=u(300001,700000)" id="op4"/>
